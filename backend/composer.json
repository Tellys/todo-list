{
    "name": "tellys-castro/vueform-mask-upload-image-schema-json-laravel-backend",
    "description": "vueform with dinamic mask, upload and manager image and assemble form with schema format json with laravel backend",
    "author": {
        "name": "Tellys Castro",
        "email": "tellyscastro@gmail.com",
        "url": "https://www.linkedin.com/in/tellys/"
    },
  "homepage": "https://www.linkedin.com/in/tellys/",
  "version": "0.1.0",
  "license": "MIT",
  "private": false,
  "keywords": [
    "schema",
    "schemajson",
    "json",
    "upload",
    "laravel",
    "masked",
    "mask",
    "vuemask",
    "vueform",
    "vue",
    "vue3",
    "authentication",
    "login",
    "router4",
    "bootstrap5",
    "bootstrap",
    "sass loader",
    "sass",
    "css",
    "laravel backend",
    "laravel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Tellys/laravel-vueform.git"
  },
    "require": {
        "php": "^8.0.2",
        "efipay/sdk-php-apis-efi": "^1.9",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^11.0",
        "laravel/reverb": "^1.0",
        "laravel/sanctum": "^4.0",
        "laravel/telescope": "^5.2",
        "laravel/tinker": "^2.7"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
